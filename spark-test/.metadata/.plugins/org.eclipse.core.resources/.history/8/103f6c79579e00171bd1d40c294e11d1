package tablon

object fechas {
  
  
  def calculoFecha (fecha:String,num:Int) = {
    
    //val fec1 = fecha
    val ano = fecha.substring(0,4)
    val mes = fecha.substring(4,6)

    val t = ano.toString()+mes.toString()
    
    
    println(t)
    
  }
  
  
  
  def main(args: Array[String]) {
    
    calculoFecha("201707",3)
    
    //Tres meses -> 201705
    //Seis meses -> 2017
    
    
  }
    

  
}