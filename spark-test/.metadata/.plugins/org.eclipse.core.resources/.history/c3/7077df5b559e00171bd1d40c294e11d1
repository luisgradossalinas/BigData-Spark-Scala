package tablon

object fechas {
  
  
  def calculoFecha (fecha:String) = {
    
    //val fec1 = fecha
    val ano = fecha.substring(4, 1)
    val mes = fecha.substring(0, 4)

    println(ano)
    println(mes)
    
  }
  
  
  
  def main(args: Array[String]) {
    
    calculoFecha("201789")
    
    
  }
    

  
}