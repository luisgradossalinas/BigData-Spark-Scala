package tablon

object fechas {
  
  
  def calculoFecha (fecha:String) = {
    
    //val fec1 = fecha
    val ano = fecha.slice(2,2)
    val mes = fecha.slice(1, 1)

    println(ano + "-" + mes)
    

  }
  
  
  
  def main(args: Array[String]) {
    
    calculoFecha("201701")
    
    
  }
    

  
}