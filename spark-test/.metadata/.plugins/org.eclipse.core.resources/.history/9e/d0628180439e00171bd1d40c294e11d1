package tablon

object fechas {
  
  
  def calculoFecha (fecha:String) = {
    
    val ano = fecha.substring(0, 4).toString()
    val mes = fecha.substring(2,2).toString()

    println(ano + "-" + mes)
    

  }
  
  
  
  def main(args: Array[String]) {
    
    calculoFecha("201701")
    
    
  }
    

  
}