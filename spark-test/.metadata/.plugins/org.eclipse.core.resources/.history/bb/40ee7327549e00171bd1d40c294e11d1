package tablon

object fechas {
  
  
  def calculoFecha (fecha:String) = {
    
    //val fec1 = fecha
    val ano = fecha.substring(0, 4).toInt
    val mes = fecha.substring(1, 1).toInt

    println(ano)
    println(mes)
    
  }
  
  
  
  def main(args: Array[String]) {
    
    calculoFecha("201701")
    
    
  }
    

  
}